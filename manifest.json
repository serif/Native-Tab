{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Native Tab",
  "description": "Simple speed dial using native features.",
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "chrome://favicon/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'; img-src chrome://favicon; img-src chrome://ntpicon",

  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },

  "version": "0.0.2",

  "options_page": "options.html",

  "offline_enabled": true,

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
